MOV     CH,[BX]             89     1             ;BX指定的内存单元到寄存器 [BX]->AL
MOV     DL,[BX]             8A     1             ;BX指定的内存单元到寄存器 [BX]->AL
MOV     DH,[BX]             8B     1             ;BX指定的内存单元到寄存器 [BX]->AL

MOV     AL,*                90     2             ;立即字节到寄存器AL
MOV     AH,*                91     2             ;立即字节到寄存器AH
MOV     BL,*                92     2             ;立即字节到寄存器BL
MOV     BH,*                93     2             ;立即字节到寄存器BH
MOV     CL,*                94     2             ;立即字节到寄存器CL
MOV     CH,*                95     2             ;立即字节到寄存器CH
MOV     DL,*                96     2             ;立即字节到寄存器DL
MOV     DH,*                97     2             ;立即字节到寄存器DH
MOV     AX,*                98     3             ;立即字到寄存器AX
MOV     BX,*                9A     3             ;立即字到寄存器BX
MOV     CX,*                9C     3             ;立即字到寄存器CX
MOV     DX,*                9D     3             ;立即字到寄存器DX
MOV     SP,*                9E     3             ;立即字到堆栈指针SP

ADD     AX,BX               A0     1             ;算术运算 AX+BX->AX
ADD     AL,BL               A1     1             ;算术运算 AL+BL->AL
ADC     AX,BX               A2     1             ;算术运算 AX+BX+C->AX
ADC     AL,BL               A3     1             ;算术运算 AL+BL+C->AL
SUB     AX,BX               A4     1             ;算术运算 AX-BX->AX
SUB     AL,BL               A5     1             ;算术运算 AL-BL->AL
SBB     AX,BX               A6     1             ;算术运算 AX-BX-C->AX
SBB     AL,BL               A7     1             ;算术运算 AL-BL-C->AL
AND     AX,BX               A8     1             ;逻辑运算 AX&BX->AX
AND     AL,BL               A9     1             ;逻辑运算 AL&BL->AL
OR      AX,BX               AA     1             ;逻辑运算 AX|BX->AX
OR      AL,BL               AB     1             ;逻辑运算 AL|BL->AL
CMP     AX,BX               AC     1             ;比较,影响标志 AX-BX
CMP     AL,BL               AD     1             ;比较,影响标志 AL-BL
NOT     AX                  B0     1             ;逻辑运算 ~AX->AX
NOT     AL                  B1     1             ;逻辑运算 ~AL->AL
INC     AX                  B2     1             ;算术运算 AX+1->AX
INC     AL                  B3     1             ;算术运算 AL+1->AL
DEC     AX                  B4     1             ;算术运算 AX-1->AX
DEC     AL                  B5     1             ;算术运算 AL-1->AL
ROL     AX                  B6     1             ;移位 AX左移
ROL     AL                  B7     1             ;移位 AL左移
ROR     AX                  B8     1             ;移位 AX右移
ROR     AL                  B9     1             ;移位 AL右移
RCL     AX                  BA     1             ;移位 AX带进位左移
RCL     AL                  BB     1             ;移位 AL带进位左移
RCR     AX                  BC     1             ;移位 AX带进位右移
RCR     AL                  BD     1             ;移位 AL带进位右移

PUSH    AX                  C1     1             ;入栈 SP-2->SP,SP->AR,AX->RAM
PUSH    BX                  C3     1             ;入栈 SP-2->SP,SP->AR,BX->RAM
PUSH    CX                  C0     1             ;入栈 SP-2->SP,SP->AR,CX->RAM
PUSH    DX                  C2     1             ;入栈 SP-2->SP,SP->AR,DX->RAM
POP     AX                  C5     1             ;出栈 SP->AR,RAM->AX,SP+2->SP
POP     BX                  C7     1             ;出栈 SP->AR,RAM->BX,SP+2->SP
POP     CX                  C4     1             ;出栈 SP->AR,RAM->CX,SP+2->SP
POP     DX                  C6     1             ;出栈 SP->AR,RAM->DX,SP+2->SP
